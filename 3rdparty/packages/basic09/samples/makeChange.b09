PROCEDURE makeChange1DIM p,n,d,q,c,t:INTEGERc:=0FOR p:=0 TO 100 STEP 5FOR n:=0 TO 20FOR d:=0 TO 10FOR q:=0 TO 4IF p+5*n+10*d+25*q=100 THEN PRINT "p="; p; " n="; n; " d="; d; " q="; qc:=c+1ENDIF NEXT qNEXT dNEXT nNEXT pPRINT PRINT c; " ways to make change for a dollar"PRINT PROCEDURE makeChange2DIM p,n,d,q,c:INTEGERc:=0FOR p:=0 TO 100 STEP 5FOR n:=0 TO 100-p STEP 5FOR d:=0 TO 100-(p+n) STEP 10FOR q:=0 TO 100-(p+n+d) STEP 25IF p+n+d+q=100 THEN PRINT "p="; p; " n="; n/5; " d="; d/10; " q="; q/25c:=c+1ENDIF NEXT qNEXT dNEXT nNEXT pPRINT PRINT c; " ways to make change for a dollar"PROCEDURE makeChange3DIM n,d,q,c:INTEGERc:=0FOR n:=0 TO 100 STEP 5FOR d:=0 TO 100-n STEP 10FOR q=0 TO 100-(n+d) STEP 25c:=c+1NEXT qNEXT dNEXT nPRINT PRINT c; " ways to make $1.00 from change"PRINT 